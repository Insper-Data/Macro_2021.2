arrange(desc(contribuicao)) %>%
head(n = top)
get_driver <- function(Phi, questoes, drv, top) {
tibble(numero = rownames(Phi), carga = Phi[, drv]) %>%
left_join(questoes) %>%
mutate(contribuicao = carga^2 / sum(carga^2)) %>%
arrange(desc(contribuicao)) %>%
head(n = top)
}
